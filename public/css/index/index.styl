body
    margin 0
    
.FS-index-wrapper
    width 100%
    height 100vh
    position relative
    
.FS-index-background
    width 100%
    height 100%
    position absolute
    background #f1f1f1
    
.FS-index-center-wrapper
    width 700px
    height 450px
    background white
    transform translate(-50%, -50%)
    top 50%
    left 50%
    position absolute
    overflow hidden
    border-radius 5px
    -webkit-box-shadow: 0px 0px 73px -1px rgba(0,0,0,0.2);
    -moz-box-shadow: 0px 0px 73px -1px rgba(0,0,0,0.2);
    box-shadow: 0px 0px 73px -1px rgba(0,0,0,0.2);
   
.FS-index-login-top
    width 100%
    height calc(100% - 75px)
    position relative
    
.FS-index-login-bottom
    width 100%
    height 75px
    position relative
    display -webkit-box
    
.FS-index-login-left, .FS-index-login-right
    width 50%
    height 100%
    position relative
    input
        width 100%
        height 100%
        background #f1f1f1
        cursor pointer
        border none
        outline none
        color #5a5a5a
        font-family helvetica
        font-size 15px
        letter-spacing 2px
        text-transform uppercase
        text-align left
        padding-left 10%
        
        
.FS-index-login-right
    input
        text-align right
        background #4996ff
        padding-right 10%
        color white
        
.FS-index-login-top-container
    width 80%
    height 80%
    transform translate(-50%, -50%)
    top 50%
    left 50%
    position absolute
    
.FS-index-login-top-container-header
    width 100%
    height 75px
    position relative
    
.FS-index-login-top-container-header-cont
    width fit-content
    height fit-content
    transform translateY(-50%)
    top 50%
    position absolute
    h1, p
        font-family helvetica
        margin 0
        text-transform uppercase
    h1
        letter-spacing: 1px;
        color: #353535;
        font-weight: 400;
    p
        color: #9e9e9e;
        margin-top 5px
        
.FS-index-login-top-container-body
    width 100%
    height calc(100% - 75px)
    position relative
    
.FS-index-login-form-wrapper
    width 100%
    height fit-content
    transform translateY(-50%)
    top 50%
    position absolute
    
.FS-index-form-object
    width 100%
    height fit-content
    position relative
    p
        font-family: helvetica;
        margin: 0;
        text-transform: uppercase;
        letter-spacing: 1.5px;
        font-weight: 100;
        font-size: 14px;
        left: 30px;
        top: 34px;
        position: relative;
        z-index 1
        transition .3s
        &.active
            left 0px
            top 0px
            
        
    input
        width calc(100% - 30px)
        height 50px
        border none
        border-bottom 1px solid gray
        font-family helvetica
        background transparent
        font-size 14px
        font-weight 300
        outline none
        padding-left 30px
        z-index 10
        position relative

.FS-index-FGP
    width 100%
    height fit-content
    margin-top 40px
    position relative
    p
        font-family: helvetica;
        margin: 0;
        text-transform: uppercase;
        letter-spacing: 1.5px;
        font-weight: 100;
        font-size: 14px;
        text-align center
        cursor pointer
    
        
.FS-top-center-branch-logo
    width 100%
    height fit-content
    position absolute
    top 0px
    margin-top 40px
    z-index 10
    
.branch-logo-FS
    width 240px
    height 62.5px
    background-image url('/img/branchCON.png')
    background-size cover
    background-position center
    background-repeat no-repeat
    margin 0 auto